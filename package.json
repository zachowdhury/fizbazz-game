  "description": "demo fizz buzz api and client",
  "main": "index.js",
  "config": {
    "port": "3000"
  },
  "scripts": {
    "client": "webpack-dev-server --mode development ",
    "server": "nodemon api/server.js --exec babel-node",
    "_comment": "dev: webpack-dev-server --open --mode development ",
